<html>
//set up canvas and width and length
function setup() {
  createCanvas(600, 400);
}
//allows you to use the background edit or create something im it
function draw() {
  //background color
  background(0);
  //makes the lines thicker/thin
  strokeWeight(4);
  //set stroke
  stroke(255);
  //for loop setting up the vars to repeat 50 times
  for (var x = 0; x <= width / 2; x += 50) {
    for (var y = 0; y <= height / 2; y += 50) {
      //color filler
      fill(0, 91, 239);
      //a shape or circle
      ellipse(x, y, 25, 25);
    }
  }
}
</html>
